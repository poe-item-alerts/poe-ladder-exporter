{
  "StartAt": "GetCharacters",
  "States": {
    "GetCharacters": {
      "Type": "Task",
      "Resource": "${lambda_arn}",
      "Next": "WaitForRateLimit"
    },
    "WaitForRateLimit": {
      "Type": "Wait",
      "Seconds": 60,
      "Next": "LoopFeedsChoice"
    },
    "LoopFeedsChoice": {
      "Type": "Choice",
      "Choices": [{
        "Variable": "$.characters",
        "NumericEquals": -1,
        "Next": "End"
      }],
      "Default": "GetCharacters"
    },
    "End": {
      "Type": "Pass",
      "Result": "All characters have been processed",
      "End": true
    }
  }
}
